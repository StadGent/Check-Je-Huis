<div class="container-fluid">
    <div class="row">
        <div class="col-md-6">
            <h1>Huidige toestand</h1>
            {% for line in calculator.current.log.lines %}
                <ul>
                    <li>{{ line }}</li>
                </ul>
            {% endfor %}
        </div>
        <div class="col-md-6">
            <h1>Gewenste toestand</h1>
            {% for line in calculator.upgrade.log.lines %}
                <ul>
                    <li>{{ line }}</li>
                </ul>
            {% endfor %}
        </div>
    </div>
</div>
